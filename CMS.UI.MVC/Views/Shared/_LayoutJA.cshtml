    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, shrink-to-fit=no">

    <!--begin::Web font -->
    <script src="@Url.Content("~/Content/Main/assets/app/js/webfont.js")"></script>
    <script>
        WebFont.load({
            google: { "families": ["Poppins:300,400,500,600,700", "Roboto:300,400,500,600,700"] },
            active: function () {
                sessionStorage.fonts = true;
            }
        });
    </script>
    <link href="@Url.Content("~/Content/Main/assets/vendors/base/vendors.bundle.css")" rel="stylesheet" type="text/css" />

